ffmpeg -i 1.mp4 -profile:v baseline -level 3.0 -s 640x360 -start_number 0 -hls_time 10 -hls_list_size 0 -f hls index.m3u8 > o.txt


ffmpeg -i 1.mp4 -profile:v baseline -start_number 0 -hls_time 10 -hls_list_size 0 -f hls vid.m3u8 

ffmpeg -i 1.mp4 -codec: copy -start_number 0 -hls_time 10 -hls_list_size 0 -f hls vid@cp.m3u8

ffmpeg -i 1.mp4 -codec: copy -start_number 0 -hls_time 20 -hls_list_size 0 -f hls v.m3u8u